{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "lib": [
            "dom",
            "es6"
        ],
        "allowJs": true,
        "downlevelIteration": true,
    },
    "include": [
        "./src/**/*"
    ],
    "files": ["./custom.d.ts"]
}